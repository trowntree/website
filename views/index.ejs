      #home p {
        font-size: 1.25rem;
        max-width: 90%;
        margin-left: auto;
        margin-right: auto;
      }
      #home .flex {
        flex-direction: column;
        align-items: center;
        justify-content: center;
        gap: 1rem;
      }
      #home .flex a {
        width: 280px;
        min-width: 280px;
        padding: 0.75rem 1rem;
        margin: 0 auto;
        box-sizing: border-box;
        text-align: center;
        font-size: 1rem;
      }
      #home .hero-media iframe {
        width: 500%; /* Increased to ensure full-screen coverage */
        height: 500%;
        object-fit: cover;
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
      }
      .card {
        padding: 1.25rem;
      }
    }
    @media (max-width: 640px) {
      .typed-text {
        font-size: 1.75rem;
        line-height: 1.4;
      }
      /* #home h1 {
        font-size: 1.75rem;
